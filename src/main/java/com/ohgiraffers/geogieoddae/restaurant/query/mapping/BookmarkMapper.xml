<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ohgiraffers.geogieoddae.restaurant.query.mapping.BookmarkMapper">
    <!-- 특정 식당의 리뷰 목록 -->
    <select id="findBookmarksByUserId"
            resultType="map">
        SELECT
            b.user_code,
            b.restaurant_code
        FROM bookmark b
        WHERE  b.user_code = #{userCode}
    </select>


</mapper>
